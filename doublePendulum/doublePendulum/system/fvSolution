FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver           GAMG;
        smoother         DICGaussSeidel;
        tolerance        1e-6;
        relTol           0.01;
    }

    pFinal
    {
        $p;
        relTol          0;
    }

    U
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-05;
        relTol          0.1;
    }

    UFinal
    {
        $U;
        relTol          0;
    }

    "pcorr.*"
    {
        tolerance    1e2;
        relTol    0.0;
//		minIter    1;
        solver    PCG;
        preconditioner    DIC;
    }
}

PIMPLE
{
    momentumPredictor    off;
    nNonOrthogonalCorrectors 0;
    nCorrectors         0;
	pRefCell    0;
	pRefValue    0;
}


